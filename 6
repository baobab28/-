#include <stdio.h>
#include <math.h>

typedef struct {
    int x;
    int y;
} Point;

int main() {
    Point p1, p2;
    
    printf("Введіть координати x1, y1, x2, y2 через пробіл: ");
    scanf("%d %d %d %d", &p1.x, &p1.y, &p2.x, &p2.y);
    
    int dx = p2.x - p1.x;
    int dy = p2.y - p1.y;

    double length = sqrt(dx * dx + dy * dy);

    printf("Довжина вектора: %.2f\n", length);
    
    return 0;
}
